{
    "extends": "../../tsconfig.json",
    "allowJs": true,
    "declaration": true,
    "declarationDir": "./dist",
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "removeComments": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "compilerOptions": {
      "outDir": "./dist",
      "rootDir": "./src"
    },
    "include": ["src"]
  }