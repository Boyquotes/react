            "types": "./dist/index.d.ts"
        },
        "./hooks": {
            "import": "./dist/hooks/index.js",
            "types": "./dist/hooks/index.d.ts"
        },
        "./utils": {
            "import": "./dist/utils/index.js",
            "types": "./dist/utils/index.d.ts"
        }
    },
    "peerDependencies": {
        "playcanvas": "^2.3.3",
        "react": ">=19.0.0",
        "react-dom": ">=19.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@types/react": "^19.0.2",
        "@types/react-reconciler": "^0.28.9",
        "typescript": "^5.6.3"
    },
    "dependencies": {
        "its-fine": "^1.2.5",
